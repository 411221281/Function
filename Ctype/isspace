//  自行編輯  //
//  判斷是否為空白字元  //

#include <stdio.h>
#include <ctype.h>

int main() {

   char ch;
   scanf("%c", &ch);

   if(isspace(ch)) {    //  isspace(ch)判斷若ch為空白 則回傳1 否則回傳0  //
       printf("%c is a white-space character", ch);
   }
   else {
       printf("%c is not a white-space character", ch);
   }
   
   return 0;
}
