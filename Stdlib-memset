#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main() { 
    int end, start = 0, count = 0, maxlen = 0, record[10001];
    char str[1000001];

    scanf("%s", &str);
    int len = strlen(str); 

    for(int i = 0; i < len; i++) {
        count = 0;

        for(int j = i; j < len; j++) { 
            if(record[str[j]] == 1) {
                count = 0;
                break;
            }

            if(record[str[j]] == 0) {
                record[str[j]] ++;
            }

            count++;
            if(count > maxlen) { 
                maxlen = count;
                start = i;
                end = maxlen + start;
            }
        }

        memset(record, 0, sizeof(record));
        
    }

    for(int i = start; i < end; i++) { 
        printf("%c", str[i]);
    }
    
    return 0;
}
