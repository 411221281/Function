#include <stdio.h>
#include <math.h>

char isPrime(int x) {

    if(x <= 1) {
        return 'N';
    }

    // 利用平方根縮短判斷時間
    for(int i = 2; i <= sqrt(x); i++) {
        if(x % i == 0) {
            return 'N';
        }
    }
    return 'Y';
    
}

int main() {

    int n, num;
    scanf("%d", &n);

    for(int i = 0; i < n; i++) {
        scanf("%d", &num);
        printf("%c\n", isPrime(num));
    }
}
