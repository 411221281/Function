#include <stdio.h>
#include <ctype.h>
#include <string.h>

int main() {
    
    char str[10001];
    int sum = 0;
    int base = 1;

    scanf("%s", &str);

    for(int i = strlen(str) -1; i >= 0; i--) {
        if(isdigit(str[i])) { 
            sum = sum + (str[i] - '0')*base;
            base *= 10;
            continue;
        }
        base = 1;
    }
    
    printf("%d", sum);
    return 0;
}
