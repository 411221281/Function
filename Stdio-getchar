#include <stdio.h>
#include <string.h>

int main() {

    char str[1000];
    int i = 0;

    while(1) {
        str[i] = getchar();
        
        if(str[i] == '\n' || str[i] == EOF) {
            break;
        }
        i++;  
    }

    int len = strlen(str);

    for(int j = 0; j < len; j++) {
        printf("%c", str[j]);
    }
    return 0;
}
