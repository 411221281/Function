// 自行編輯 //
// 輸出含空格之字串 //

#include <stdio.h>
#include <string.h>

int main() {

    char str[1000];
    int i = 0;

    while(1) {
        str[i] = getchar(); // getchar 不會因為空格建 造成字串截斷 只取空格前之字串 與scanf的差異在此 //
        
        if(str[i] == '\n' || str[i] == EOF) {
            break;
        }
        i++;  
    }

    int len = strlen(str);

    for(int j = 0; j < len; j++) {
        printf("%c", str[j]);
    }
    return 0;
}
