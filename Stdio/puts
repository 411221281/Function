//  自行編輯  //
//  用puts()輸出字串  //

#include <stdio.h>
#include <string.h>

int main () {

   char str1[100];
   char str2[100];

   strcpy(str1, "Hello World");
   strcpy(str2, "Nice To Meet You");

   puts(str1);  //  puts和printf的差異為 puts()函數會自動在字串尾部加換行符號 而printf()沒有此功能  //
   puts(str2);
   
   return 0;
}
