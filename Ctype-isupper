//  自行編輯  //
//  判斷英文字母是否為大寫  //

#include <stdio.h>
#include <ctype.h>

int main () {
   char ch;
   scanf("%c", &ch);
    
   if(isupper(ch)) {  //  isupper() 是用來判斷一字母是否為大寫  //
      printf("%c is uppercase character\n", ch);
   } 
   else {
      printf("%c is not uppercase character\n", ch);
   }

   return 0;
}
